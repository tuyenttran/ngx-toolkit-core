        "test": "npm run lint && ng test --watch false",
        "test-watch": "ng test",
        "e2e": "ng e2e",
        "e2e-watch": "watch \"ng e2e -s false\" src e2e --wait=1",
        "lint": "ng lint",
        "publish": "npm run build-lib && npm publish --access=public dist/ui4ngx/core/ --registry https://registry.npmjs.org/"
